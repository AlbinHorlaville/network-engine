{
  "openapi": "3.0.1",
  "info": {
    "title": "My API",
    "version": "v1"
  },
  "paths": {
    "/Achievements": {
      "get": {
        "tags": [
          "Achievements"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Auth/register": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Auth/login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Matchmaking/queue": {
      "post": {
        "tags": [
          "Matchmaking"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Matchmaking/unqueue": {
      "post": {
        "tags": [
          "Matchmaking"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Matchmaking/register": {
      "post": {
        "tags": [
          "Matchmaking"
        ],
        "parameters": [
          {
            "name": "ip",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Matchmaking/unregister": {
      "post": {
        "tags": [
          "Matchmaking"
        ],
        "parameters": [
          {
            "name": "ip",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Matchmaking/status": {
      "get": {
        "tags": [
          "Matchmaking"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Stats": {
      "get": {
        "tags": [
          "Stats"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Stats/{username}": {
      "post": {
        "tags": [
          "Stats"
        ],
        "parameters": [
          {
            "name": "username",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PlayerStats"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/PlayerStats"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/PlayerStats"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LoginRequest": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PlayerStats": {
        "type": "object",
        "properties": {
          "gamesWon": {
            "type": "integer",
            "format": "int32"
          },
          "cubesPushed": {
            "type": "integer",
            "format": "int32"
          },
          "maxCubesPushedInOneGame": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "Enter 'Bearer' [space] and then your token",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}